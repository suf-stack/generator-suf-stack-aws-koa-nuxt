{
  "name": "koa-test-init",
  "version": "1.0.0",
  "description": "My exquisite Nuxt.js project",
  "author": "Brad Ito <brad@retina.ai>",
  "private": true,
  "scripts": {
    "analyze": "nuxt build --config-file ./nuxt.config.js --analyze",
    "dev": "cross-env NODE_ENV=development nodemon server_local.js --watch src-koa",
    "build-dev": "nuxt build --config-file ./nuxt.config.js",
    "build-prod": "nuxt build --config-file ./nuxt.config.prod.js && npm run rollup",
    "build-staging": "nuxt build --config-file ./nuxt.config.staging.js && npm run rollup",
    "gulp-prod": "gulp publish_static_prod",
    "gulp-staging": "gulp publish_static_staging",
    "deploy-prod": "npm run build-prod && npm run gulp-prod && serverless deploy --verbose --stage prod",
    "deploy-staging": "npm run build-staging && npm run gulp-staging && serverless deploy --verbose --stage staging",
    "rollup": "rollup -c",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "engines": {
    "node": ">= 10.13"
  },
  "dependencies": {
    "@koa/router": "^8.0.5",
    "@nuxtjs/axios": "^5.0.0",
    "bunyan": "^1.8.12",
    "cross-env": "^5.2.0",
    "koa": "^2.5.2",
    "nuxt-start": "^2.11.0",
    "serverless-http": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-awspublish": "^4.1.0",
    "gulp-rename": "^2.0.0",
    "nodemon": "^1.11.0",
    "nuxt": "^2.11.0",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy-assets": "^2.0.1",
    "rollup-plugin-json": "^4.0.0",
    "serverless": "^1.60.4"
  }
}
